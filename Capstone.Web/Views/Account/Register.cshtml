
@model Capstone.Web.Models.RegistrationForm

<title>Register</title>

<form id="registerform" method="post" action="@Url.Action("Register", "Account")">
    <div id="username">
        @Html.LabelFor(m => m.UserName)
        @Html.TextBoxFor(m => m.UserName, new { id = "username" })
    </div>
    <div id="fName">
        @Html.LabelFor(m => m.FirstName)
        @Html.TextBoxFor(m => m.FirstName,new { id="fName"})
    </div>
    <div id="#lName">
        @Html.LabelFor(m => m.LastName)
        @Html.TextBoxFor(m => m.LastName, new { id = "lName" })
    </div>
    <div id="#email">
        @Html.LabelFor(m => m.Email)
        @Html.TextBoxFor(m => m.Email, new { id = "email",})
    </div>
    <div id="#password">
        @Html.LabelFor(m => m.Password)
        @Html.PasswordFor(m=> m.Password, new { id="password" })
    </div>
    <br />
    <input type="submit" name="submitButton" value="Submit Form" />
</form>